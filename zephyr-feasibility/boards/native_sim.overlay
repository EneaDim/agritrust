&i2c0 {
    status = "okay";
    compatible = "zephyr,i2c-emul-controller";
    
    temp_hum_emul: sht3xd@44 {
        compatible = "sensirion,sht3xd";
        reg = <0x44>;
        status = "okay";
    };

    light_emul: bh1750@23 {
        compatible = "rohm,bh1750";
        reg = <0x23>;
        status = "okay";
    };
};

&gpio0 {
    status = "okay";
};

